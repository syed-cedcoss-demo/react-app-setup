{
  "name": "react-app-setup",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f ./envs/.env.development react-scripts start",
    "build:development": "env-cmd -f ./envs/.env.development react-scripts build",
    "build:staging": "env-cmd -f ./envs/.env.staging react-scripts build",
    "build:production": "env-cmd -f ./envs/.env.production react-scripts build",
    "prepare": "husky install",
    "lint": "eslint --fix ."
  },
  "author": "Syed Hasnain Mehadi",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "^20.6.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.2.7",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "redux-persist": "^6.0.0",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "engineStrict": true,
  "engines": {
    "npm": ">=8.0.0 <9.8.1",
    "node": ">=16.0.0 <18.18.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^8.0.0"
  }
}
